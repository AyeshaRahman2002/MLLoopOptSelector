# .gitignore (Python)
cat > .gitignore <<'EOF'
# Python
__pycache__/
*.py[cod]
*.egg-info/
.env
.venv/
venv/
.envrc

# Jupyter
.ipynb_checkpoints/

# macOS
.DS_Store
EOF

# remove from Git history (keep locally)
git rm -r --cached .venv __pycache__ src/__pycache__ .ipynb_checkpoints || true
git add .gitignore
git commit -m "Cleanup: ignore venv and caches"
git push
